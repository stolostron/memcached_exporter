include Makefile.common

# Override common-build target to build dynamically linked binary
.PHONY: common-build
common-build: promu
	@echo ">> building dynamic binaries"
	$(PROMU) -c ".promu.dynamic.yaml" build -v --cgo --prefix $(PREFIX) $(PROMU_BINARIES)
